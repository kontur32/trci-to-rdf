{
  "schema":{
    "context":{
      "aliases":{
        "парсер-нормализация-пробелов":{
          "xquery":"normalize-space(.)"
        }
      },
      "parameters":{
        "домен.сущности":"{{домен.сущности}}",
        "домен.сущности.строки":"{{домен.сущности}}строки"
      }
    },
    "table":{
      "type":"resource",
      "filter":"^Кодификатор",
      "properties":[
        {
          "type":"property",
          "nameSpace":"{{домен.схема}}",
          "localName":"{{домен.схема.префикс}}:дата",
          "value":{
            "xquery":"current-dateTime()"
          }
        },
        {
          "type":"property",
          "nameSpace":"{{домен.схема}}",
          "localName":"{{домен.схема.префикс}}:label",
          "value":{
            "xquery":".\/table\/@label\/data()"
          }
        },
        {
          "type":"property",
          "nameSpace":"{{xmlns.rdf}}",
          "localName":"rdf:type",
          "value":"{{домен.онтология}}{{class.table}}"
        }
      ],
      "row":{
        "type":"resource",
        "about":{
          "value":{
            "xquery":"'{{домен.схема}}' || random:uuid()"
          }
        },
        "nameSpace":"{{домен.схема}}",
        "localName":"{{домен.схема.префикс}}:строка",
        "filter":{
          "value":{
            "xquery":"."
          }
        },
        "properties":[
          {
            "type":"property",
            "nameSpace":"{{xmlns.rdf}}",
            "localName":"rdf:type",
            "value":"{{домен.онтология}}{{class.row}}"
          }
        ],
        "cell":{
          "properties":[
            {
              "filter":"^Предметные области",
              "type":"resource",
              "nameSpace":"{{домен.схема}}",
              "localName":"{{домен.схема.префикс}}:свойство-1",
              "value":{
                "alias":"парсер-нормализация-пробелов"
              },
              "validator":{
                "alias":"валидация-текста"
              },
              "properties":[
                {
                  "type":"property",
                  "nameSpace":"{{домен.схема}}",
                  "localName":"{{домен.схема.префикс}}:label",
                  "value":{
                    "xquery":".\/@label\/data()"
                  }
                },
                {
                  "type":"property",
                  "nameSpace":"{{домен.схема}}",
                  "localName":"{{домен.схема.префикс}}:значение",
                  "value":{
                    "xquery":".\/text()"
                  }
                }
              ]
            },
            {
              "filter":"^Учебные предметы",
              "type":"resource",
              "nameSpace":"{{домен.схема}}",
              "localName":"{{домен.схема.префикс}}:свойство-2",
              "value":{
                "alias":"парсер-нормализация-пробелов"
              },
              "validator":{
                "alias":"валидация-текста"
              },
              "properties":[
                {
                  "type":"property",
                  "nameSpace":"{{домен.схема}}",
                  "localName":"{{домен.схема.префикс}}:label",
                  "value":{
                    "xquery":".\/@label\/data()"
                  }
                },
                {
                  "type":"property",
                  "nameSpace":"{{домен.схема}}",
                  "localName":"{{домен.схема.префикс}}:значение",
                  "value":{
                    "xquery":".\/text()"
                  }
                }
              ]
            },
            {
              "filter":"^Код предмета в УП",
              "type":"resource",
              "nameSpace":"{{домен.схема}}",
              "localName":"{{домен.схема.префикс}}:свойство-3",
              "value":{
                "alias":"парсер-нормализация-пробелов"
              },
              "validator":{
                "alias":"валидация-текста"
              },
              "properties":[
                {
                  "type":"property",
                  "nameSpace":"{{домен.схема}}",
                  "localName":"{{домен.схема.префикс}}:label",
                  "value":{
                    "xquery":".\/@label\/data()"
                  }
                },
                {
                  "type":"property",
                  "nameSpace":"{{домен.схема}}",
                  "localName":"{{домен.схема.префикс}}:значение",
                  "value":{
                    "xquery":".\/text()"
                  }
                }
              ]
            }
          ]
        }
      }
    }
  }
}