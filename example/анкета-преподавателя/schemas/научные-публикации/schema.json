{
    "schema":{
      "context":{
        "aliases":{
          "парсер-нормализация-пробелов":{
            "xquery":"normalize-space(.)"
          }
        },
        "parameters":{
          "RDF-endpoint": "{{root.RDF-endpoint}}",
          "ФИО":{
            "value":{
              "xquery": "./file/table[1]/row/cell[@label='ФИО']/text()"
            }
          }
        }
      },
      "table":{
        "type":"resource",
        "filter": "^Научные",
        "context": [
          {
            "nameSpace":"",
            "localName":"ФИО",
            "value": {
              "xquery": "./parameters/ФИО/text()"
            }
          }
        ],
        "about":{
          "value":{
            "sparql": {"URL":"{{root.семантический.контракт}}about-by-fio.rq"}
          }
        },
        "row":{
          "type":"resource",
          "context": [
            {
              "nameSpace":"",
              "localName":"ФИО",
              "value": {
                "xquery": "./parameters/ФИО/text()"
              }
            }
          ],
          "about":{
            "value":{
              "xquery": "'{{root.схема}}{{root.сущности}}НаучныеПубликации#' || random:uuid()"
            }
          },
          "nameSpace":"{{root.схема}}",
          "localName":"{{root.префикс}}:{{local.class.row}}",
          "filter":{
            "value":{
              "xquery":"."
            }
          },
          "properties":[
            {
              "type":"resource",
              "nameSpace":"{{root.xmlns.rdf}}",
              "localName":"rdf:type",
              "value":"{{root.схема}}{{root.онтология}}{{local.class.row}}"
            },
            {
              "nameSpace":"https://schema.org/",
              "localName":"sch:DateTime",
              "value":{
                "xquery":"current-dateTime()"
              }
            },
            {
              "nameSpace":"{{root.схема}}",
              "localName":"{{root.префикс}}:ФИО",
              "value":{
                "xquery":"./parameters/ФИО/text()"
              }
            }
          ],
          "cell":{
            "properties":[
              {
                "filter":"^Индекс",
                "nameSpace":"{{root.схема}}",
                "localName":"{{root.префикс}}:наличиеВИндексеЦитирований",
                "value":{
                  "alias":"парсер-нормализация-пробелов"
                },
                "validator":{
                  "alias":"валидация-текста"
                }
              },
              {
                "filter":"^Квартиль",
                "nameSpace":"{{root.схема}}",
                "localName":"{{root.префикс}}:квартильВИндексеЦитирований",
                "value":{
                  "alias":"парсер-нормализация-пробелов"
                },
                "validator":{
                  "alias":"валидация-текста"
                }
              },
              {
                "filter":"^Год публикации",
                "nameSpace":"{{root.схема}}",
                "localName":"{{root.префикс}}:годПубликации",
                "value":{
                  "alias":"парсер-нормализация-пробелов"
                },
                "validator":{
                  "alias":"валидация-текста"
                }
              },
              {
                "filter":"^Публикация",
                "nameSpace":"{{root.схема}}",
                "localName":"{{root.префикс}}:библиографическаяЗапись",
                "value":{
                  "alias":"парсер-нормализация-пробелов"
                },
                "validator":{
                  "alias":"валидация-текста"
                }
              },
              {
                "filter":"^Ссылка",
                "type":"resource",
                "nameSpace":"{{root.схема}}",
                "localName":"{{root.префикс}}:текстПубликации",
                "value":{
                  "alias":"парсер-нормализация-пробелов"
                },
                "validator":{
                  "alias":"валидация-текста"
                },
                "properties":[
                  {
                    "type":"property",
                    "nameSpace":"{{root.схема}}",
                    "localName":"{{root.префикс}}:тип",
                    "value": "ссылка"
                  },
                  {
                    "type":"property",
                    "nameSpace":"{{root.схема}}",
                    "localName":"{{root.префикс}}:URL",
                    "value":{
                      "xquery":".\/text()"
                    }
                  }
                ]
              },
              {
                "filter":"^DOI",
                "nameSpace":"{{root.схема}}",
                "localName":"{{root.префикс}}:DOI",
                "value":{
                  "alias":"парсер-нормализация-пробелов"
                },
                "validator":{
                  "alias":"валидация-текста"
                }
              }
            ]
          }
        }
      }
    }
  }